{
  "name": "server",
  "version": "1.0.0",
  "description": "My cool TypeScript app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "generate-swagger": "node generate-swagger.js",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@types/swagger-ui-express": "^4.1.6",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "groq-sdk": "^0.7.0",
    "pino": "^7.8.1",
    "swagger-express-middleware": "^4.0.2",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.21",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.45",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-express-middleware": "^1.0.16",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.3",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.5",
    "supertest": "^6.1.6",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
